-- use at your own risk
local part = workspace.Finish.Finish
local player = game.Players.LocalPlayer

local function startTouchSpam(character)
    local hum = character:WaitForChild("HumanoidRootPart")
    if not hum then
        return
    end
    task.spawn(function()
        while hum.Parent do
            firetouchinterest(hum, part, 0)
            firetouchinterest(hum, part, 1)
            task.wait(0.01)
        end
    end)
end

if player.Character then
    startTouchSpam(player.Character)
end

player.CharacterAdded:Connect(function(character)
    startTouchSpam(character)
end)
