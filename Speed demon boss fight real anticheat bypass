local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/jensonhirst/Orion/main/source"))()

local Window = OrionLib:MakeWindow({Name = "Speed Demon Anticheat Bypass made by VoidOverlord", HidePremium = false, SaveConfig = false})
local Tab = Window:MakeTab({Name = "Anticheat Disabler", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local StatusLabel = Tab:AddLabel("Status: Waiting for actions...")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:FindFirstChild("PlayerGui")
local PlayerScripts = LocalPlayer:FindFirstChild("PlayerScripts")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local function preventObjectRegeneration()
    local function destroySuspiciousObjects()
        if PlayerGui then
            for _, child in pairs(PlayerGui:GetChildren()) do
                if child.Name == "fire" then
                    child:Destroy()
                end
            end
        end
        if PlayerGui then
            local ac = PlayerGui:FindFirstChild("ac")
            if ac then
                ac:Destroy()
            end
        end

        if PlayerScripts then
            local kmdl = PlayerScripts:FindFirstChild("Kmdl")
            if kmdl and kmdl:IsA("LocalScript") then
                kmdl.Disabled = true
                kmdl:Destroy()
            end
        end

        local suspicious = {"kys", "noo", "LoggerEvent", "fn", "relfx"}
        for _, name in ipairs(suspicious) do
            local found = ReplicatedStorage:FindFirstChild(name)
            if found then
                found:Destroy()
            end
        end
    end

    RunService.Heartbeat:Connect(function()
        destroySuspiciousObjects()
    end)
end

preventObjectRegeneration()

Tab:AddButton({
	Name = "Remove Anticheat",
	Callback = function()
		preventObjectRegeneration()

		StatusLabel:Set("Status: Removed Anticheat")
		OrionLib:MakeNotification({Name = "Anticheat Disabler ("Sdbfr), Content = "All client sided anticheat has been removed", Time = 3})
	end
})

local connection
local checking = true

local function antiCheatCheck(humanoid)
	if connection then connection:Disconnect() end
	connection = RunService.Heartbeat:Connect(function()
		if not checking then return end
		if humanoid:GetState() == Enum.HumanoidStateType.Swimming then
			checking = false
			connection:Disconnect()
			LocalPlayer:Kick("You have been kicked for swimming in hopes of you not being logged.")
		elseif humanoid.PlatformStand then
			checking = false
			connection:Disconnect()
			LocalPlayer:Kick("You have been kicked for using platform state to save you from being logged.")
		end
	end)
	task.spawn(function()
		task.wait()
		while checking do
			task.wait()
		end
	end)
end

local function monitorCharacter()
	local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local humanoid = character:FindFirstChild("Humanoid") or character:WaitForChild("Humanoid")
	antiCheatCheck(humanoid)
	LocalPlayer.CharacterAdded:Connect(function(newCharacter)
		if not checking then return end
		if connection then connection:Disconnect() end
		humanoid = newCharacter:WaitForChild("Humanoid")
		antiCheatCheck(humanoid)
	end)
end

monitorCharacter()
